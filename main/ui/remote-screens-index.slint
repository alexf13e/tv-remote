
//a list of the screens which exist in the remote.
//used by remote buttons specifying which screen should be switched to when clicked
//screen id is used by the main window in app-window.slint to determine which screen should be shown

export enum RemoteScreenID
{
    SAME,   //stay on the same screen (default when none is specified for a button)
    HOME,
    SCREEN_LIST,
    IR_RECEIVE,
    TV_ALL_BUTTONS,
    TV_CHANNELS,
    TV_APPS,
    TV_PLAYBACK,
    SPEAKERS_ALL_BUTTONS,
    HDR_ALL_BUTTONS,
    YOUVIEW_ALL_BUTTONS,
}